/**index.wxss**/
page{
  background: #f5f5f5;
}
.container {
  position: absolute;
  width:100%;
  height: 910rpx;
  background-color: #fff;
}
.nav_left{
  position: absolute;
  top:0rpx;
  left:0rpx;
  width:25%;
  background:#f5f5f5;
  text-align: center;
}
.nav_left .nav_left_items {
  height:80rpx;
  line-height: 80rpx;
  padding: 12rpx 0;
  border-bottom: 2rpx solid #dedede;
  font-size: 28rpx;
}
.nav_left .nav_left_items.active{
  background: #fff;
  color:#3385ff;
  border-left:6rpx solid #3385ff;
}

.nav_right{
  position: absolute;
  top:0;
  left:210rpx;
  width:75%;
  height: 900rpx;
  background:#fff;
}
.nav_right .nav_right_items{
  margin-top:20rpx;
  padding: 20rpx;
  display: block;
  width:92%;
  height: 120rpx;
  line-height: 120rpx;
  background: rgb(239, 243, 245);
  overflow: hidden;
  border-radius: 10rpx;
}
.nav_right_image{
  display: inline-block;
  width: 33%;
  height: 130rpx;
  overflow: hidden;
  align-items: center;
}
.image{
  width: 130rpx;
  height: 130rpx;
border-radius: 10rpx;
}
