body {
  margin: 0px;
  padding: 0px;
}

.top {
  position: relative;
  width: 200px;
  height: 100px;
}
.top button{
  float: center;
}
button {
  width: 50px;
  height: 50px;
  /* background:  */
  border-right: 25px;
  background-color: #fff;
}

button::after {
  border: 0;
}

image {
  width: 35px;
  height: 35px;
}

.center {
  height: 200px;
  width: 200px;
}

.center-left image {
  width: 30px;
  height: 30px;
  display: block;
}

.center-left {
  float: left;
  width: 150px;
  height: 200px;
  font-family: Impact, Haettenschweiler, 'Arial Narrow Bold', sans-serif;
}

.center-left-top {
  float: top;
}

.center-left view {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 30%;
}

.center-left-bottom {
  float: bottom;
}

.center-right {
  float: right;
  width: 40px;
  height: 200px;
  font-family: Impact, Haettenschweiler, 'Arial Narrow Bold', sans-serif;
}
.center-right view{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 15%;
}
